<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jmg.android.questionario">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Questao1Activity"></activity>
        <activity android:name=".Questao2Activity"></activity>
        <activity android:name=".Questao3Activity"></activity>
        <activity android:name=".Questao4Activity"></activity>
        <activity android:name=".Questao5Activity"></activity>
        <activity android:name=".ResultadoActivity"></activity>
    </application>

</manifest>